<?xml version='1.0' encoding='utf-8'?>
<widget id="com.NetworkingWater.NWData" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Networking Water</name>
    <description>
        Data aggregator application for the monitoring of water and power supply services.
    </description>
    <author email="ricardoglass@gmail.com" href="http://networkingwater.com">
        Networking Water
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#ee6e73" />
    <preference name="StatusBarStyle" value="blacktranslucent" />
    <platform name="ios">
        <icon src="res/icon/ios/icon.png" />
    </platform>
    <engine name="ios" spec="~4.5.5" />
</widget>
